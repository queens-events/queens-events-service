{
  "name": "quevents-service",
  "version": "1.0.0",
  "description": "Queens Events backend service",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=dev node index.js",
    "test": "node node_modules/mocha/bin/mocha",
    "migrate": "sequelize --config migrationConfig.json db:migrate",
    "reset-migrate": "sequelize --config migrationConfig.json db:migrate:undo:all",
    "seed-venues": "sequelize --config migrationConfig.json db:seed --seed 20170901174119-seed_venue_data.js",
    "seed-events": "sequelize --config migrationConfig.json db:seed --seed 20170901174144-seed_event_data.js"
  },
  "repository": "https://github.com/QueensEvents/quevents-service",
  "author": "Stephen Peterkins",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "eslint-config-airbnb": "^15.1.0",
    "express": "^4.15.4",
    "express-promise-router": "^2.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mysql2": "^1.4.2",
    "sequelize": "^4.4.3",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.0"
  }
}
